server {
    listen 80;
    server_name 18.133.33.186;

    location / {
        WSGIDaemonProcess morpheus python-home=~/excelapp/.venv python-path=~/excelapp
        WSGIProcessGroup morpheus
    }

    location /static/ {
        autoindex on;
        alias    ~/morpheus/excelapp/webapp/static/;
    }
}
