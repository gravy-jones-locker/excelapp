<VirtualHost *:80>

    Alias /static ~/excelapp/webapp/static
    <Directory ~/excelapp/webapp/static>
        Require all granted
    </Directory>

    <Directory ~/excelapp/excelapp>
        <Files wsgi.py>
            Require all granted
        </Files>
    </Directory>

    WSGIDaemonProcess myproject python-home=~/excelapp/.venv python-path=~/excelapp
    WSGIProcessGroup myproject
    WSGIScriptAlias / ~/excelapp/excelapp/wsgi.py

</VirtualHost>