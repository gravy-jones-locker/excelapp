server {
    listen 80;
    server_name 127.0.0.1;

    location / {
        # django running in uWSGI
        uwsgi_pass unix:///run/uwsgi/app/django/socket;
        include uwsgi_params;
        uwsgi_read_timeout 300s;
        client_max_body_size 32m;
    }

    location /static/ {
        autoindex on;
        alias    ~/morpheus/excelapp/webapp/static/;
    }
}
